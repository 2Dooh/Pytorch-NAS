{
    "agent": "EvoAgent",
    "exp_name": "[NSGA2-101][FLOPS_VALID]-UX-G200-S00-25EP",
    "exp_cfg": {
        "seed": 0,
        "custom_problem": true, 
        "summary_writer": true
    },

    "algorithm": {
        "name": "nsga2",
        "kwargs": {
            "pop_size": 50,
            "n_offsprings": 50
        }
    },

    "eliminate_duplicates": {
        "name": "Bench101DuplicateEliminator",
        "kwargs": {}
    },
    
    "sampling": "bin_random",

    "termination": {
        "name": "n_gen",
        "args": [200]
    },

    "crossover": {
        "name": "(real|bin|int)_ux",
        "kwargs": {
            "prob": 0.9
        }
    },

    
    "mutation": {
        "name": "bin_bitflip",
        "kwargs": {
            "prob": 0.02
        }
    },

    "repair": "Bench101Repairer",

    "selector": {
        "name": "TournamentSelector",
        "kwargs": {
            "t_size": 2,
            "s_size": 50
        }
    },

    "problem": {
        "name": "Bench101",
        "kwargs": {
            "epoch": 36,
            "elementwise_evaluation": true
        }
    },

    "callbacks": {
        "PopLogger": {},
        "PopSaver": {},
        "ParetoFrontProgress": {
          "plot_freq": 1,
          "labels": ["Flops", "Valid Error"],
          "loc": "upper right"
        }
    }
}
