            "eta": 15
        }
    },

    "mutation": {
        "name": "real_pm",
        "kwargs": {
            "eta": 20
        }
    },

    "problem": {
        "name": "zdt6",
        "kwargs": {
            "n_var": 10
        }
    },

    "callbacks": {
        "PopLogger": {},
        "ParetoFrontProgress": {},
        "PopSaver": {}
    },

    "checkpoint": "experiments/ZDT4/checkpoints/[NSGA2_ZDT6] G-5.npy"
}
