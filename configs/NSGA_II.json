{
    "agent": "EvoAgent",
    "algorithm": {
        "name": "nsga2",
        "kwargs": {
            "pop_size": 100,
            "n_offsprings": 100
        }
    },

    "exp_name": "ZDT4",
    "exp_cfg": {
        "seed": 0,
        "custom_problem": false, 
        "summary_writer": true
    },
    
    "sampling": "real_random",

    "eliminate_duplicates": true,

    "termination": {
        "name": "n_gen",
        "args": [200]
    },

    "crossover": {
