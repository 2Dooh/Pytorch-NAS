{
    "agent": "NSGAII",

    "exp_cfg": {
        "seed": 0,
        "summary_writer": true
    },
    
    "initializer": {
        "name": "RandomSampling",
        "kwargs": {
            "size": 50
        }
    },


    "terminator": {
        "name": "ConditionMet",
        "kwargs": {
            "counter": "current_gen",
            "flag": 200
        }
    },

    "crossover": {
        "name": "UniformCrossover",
        "kwargs": {
            "prob": 0.9,
            "eta": 15
        }
    },

    "selector": {
        "name": "TournamentSelector",
        "kwargs": {
            "t_size": 2,
            "s_size": 50
        }
    },

    "problem": {
        "name": "NasBench201GradientFree",
        "kwargs": {
            "dataset": "cifar10",
            "max_nodes": 4,
            "input_size": [32, 3, 32, 32],
            "cuda": true
        }
    },

    "callbacks": {
        "PopLogger": {},
        "ParetoFrontProgress": {
            "plot_freq": 1,
            "labels": ["flops", "ntk + lrc"],
            "loc": "upper right"
        },
        "PopSaver": {
            "save_freq": 1
        }
    }

    
}
