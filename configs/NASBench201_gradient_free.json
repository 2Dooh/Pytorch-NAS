{
    "agent": "NSGAII",

    "exp_cfg": {
        "seed": 0,
        "summary_writer": true
    },
    
    "initializer": {
        "name": "RandomSampling",
        "kwargs": {
            "size": 50
        }
    },


    "terminator": {
        "name": "ConditionMet",
        "kwargs": {
            "counter": "current_gen",
            "flag": 40
        }
    },

    "crossover": {
        "name": "UniformCrossover",
        "kwargs": {
            "prob": 0.9,
            "eta": 15
        }
    },

    "mutator": {
        "name": "BitFlipMutator",
        "kwargs": {
            "prob": 0.02
        }
    },

    "repairer": {
        "name": "NasBench201Repairer",
        "kwargs": {
        }
    },

    "selector": {
        "name": "TournamentSelector",
        "kwargs": {
            "t_size": 2,
            "s_size": 50
        }
    },

    "problem": {
        "name": "NasBench201GradientFree",
        "kwargs": {
            "dataset": "cifar10",
            "max_nodes": 4,
            "input_size": [32, 3, 32, 32],
            "cuda": true
        }
    },

    "callbacks": {
        "PopSaver": {},
        "PopLogger": {},
        "ParetoFrontProgress": {
          "plot_freq": 1,
          "labels": ["Flops", "Neural Tangent Kernels", "Linear Regions"],
          "loc": "upper right"
        }
        
    }
}
